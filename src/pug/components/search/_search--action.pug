.search__action
	.search__action-wrapper
		.search__action-wrapper-left
			.search__action-left
				a.search__action-btn(href="#")
					i.icon-font.icon-keyboard-arrow-down

			.search__action-middle
				a.search__action-btn.active(href="#") 1
				a.search__action-btn(href="#") 2
				a.search__action-btn(href="#") 3
				a.search__action-btn(href="#") 4
				a.search__action-btn(href="#") 5
				a.search__action-btn(href="#") ...

			.search__action-right
				a.search__action-btn(href="#")
					i.icon-font.icon-keyboard-arrow-down

		.search__action-wrapper-middle

			.search__action-line

		.search__action-wrapper-right

			nav(aria-label='breadcrumb')
				ol.breadcrumb
					li.breadcrumb-item
						a(href='#') Myanmar
					li.breadcrumb-item.active(aria-current='page')
						a(href='#') Jobs


