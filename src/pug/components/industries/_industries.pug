// SECTION :: START

section#industries.industries
	.industries__wrapper.industries__wrapper-1
		.industries__banner
			p Top Industries to Work in Myanmar

	.c-grid
		.industries__wrapper.industries__wrapper-2

			.industries__box-wrapper
				-for(let i = 0; i < 48; i++)
					div
						a.industries__box(href="#")
							.industries__box-head
							.industries__box-body
								p!= "Real Estate/Property Development<span>(213)</span>"


		.industries__wrapper.companies__wrapper-3

			p.companies__title Popular Loctions
			.companies__line

			.companies__locations-wrapper
				each val, idx in [1,2,3,4,5,6,7]
					div
						a.companies__locations(href="#")
							.companies__locations-head
								img(src="img/img-jobdetails-banner.jpg" alt="")
							.companies__locations-body
								p Ayeyawady Division


// SECTION :: END
